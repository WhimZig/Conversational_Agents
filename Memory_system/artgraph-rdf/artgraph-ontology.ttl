@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://www.gennarovessio.com/artgraph-schema#Field> a owl:Class;
  rdfs:label "Field" .

<https://www.gennarovessio.com/artgraph-schema#inCountry> a owl:ObjectProperty;
  rdfs:range <https://www.gennarovessio.com/artgraph-schema#Country>;
  rdfs:domain <https://www.gennarovessio.com/artgraph-schema#City>, <https://www.gennarovessio.com/artgraph-schema#Gallery>;
  rdfs:label "inCountry" .

<https://www.gennarovessio.com/artgraph-schema#inCity> a owl:ObjectProperty;
  rdfs:range <https://www.gennarovessio.com/artgraph-schema#City>;
